{
  "name": "web",
  "chef_type": "role",
  "json_class": "Chef::Role",
  "description": "",
  "run_list": [
    "recipe[user::data_bag]",
    "recipe[sudo]",
    "recipe[openssh]",
    "recipe[iptables]",
    "recipe[ruby]",
    "recipe[mysql]",
    "recipe[database]",
    "recipe[resolv]"
  ],
  "default_attributes": {
    "users": ["sample"],
    "authorization": {
      "sudo": {
        "users": ["sample"],
        "passwordless": "true"
      }
    },
    "openssh": {
      "server": {
        "port": "22",
        "permit_root_login": "no",
        "password_authentication": "no"
      }
    },
    "ruby": {
      "version": "2.1.2"
    },
    "rbenv": {
      "group_users": ["sample"]
    },
    "mysql": {
      "version": "5.6"
    }
  },
  "override_attributes": {
  }
}
